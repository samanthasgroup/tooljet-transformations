{
  "name": "tooljet-transformations",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "format": "prettier --write ./src/**/*.js ./test/**/*.js",
    "format-check": "prettier -c ./src/**/*.js ./test/**/*.js",
    "lint": "node ./node_modules/eslint/bin/eslint . --ext .js --fix",
    "lint-check": "node ./node_modules/eslint/bin/eslint . --max-warnings=0 --ext .js",
    "test": "jest"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "eslint": "^8.40.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.5.0",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.8.8"
  }
}
